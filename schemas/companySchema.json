{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "handle",
      "name",
      "num_employees",
      "description",
      "logo_url"
    ],
    "properties": {
      "handle": {
        "$id": "#/properties/handle",
        "type": "string",
        "title": "The Handle Schema",
        "default": "",
        "examples": [
          "UCSF"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "University of Cal SF"
        ],
        "pattern": "^(.*)$"
      },
      "num_employees": {
        "$id": "#/properties/num_employees",
        "type": "integer",
        "title": "The Num_employees Schema",
        "default": 0,
        "examples": [
          3000
        ]
      },
      "description": {
        "$id": "#/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "Where Orlando used to work but he got stressed"
        ],
        "pattern": "^(.*)$"
      },
      "logo_url": {
        "$id": "#/properties/logo_url",
        "type": "null",
        "title": "The Logo_url Schema",
        "default": null,
        "examples": [
          null
        ]
      }
    }
  }